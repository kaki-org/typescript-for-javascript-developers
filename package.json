{
  "name": "typescript-for-javascript-developers",
  "version": "1.0.0",
  "description": "test",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": ["ts-jest", {
        "target": "es5",
        "module": "commonjs",
        "baseUrl": "./",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
      }]
    },
    "testMatch": [
      "**/tests/*.test.(ts|tsx|js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kakikubo/typescript-for-javascript-developers.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kakikubo/typescript-for-javascript-developers/issues"
  },
  "homepage": "https://github.com/kakikubo/typescript-for-javascript-developers#readme",
  "dependencies": {
    "axios": "1.10.0",
    "yarn": "2.4.3"
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/node": "24.0.8",
    "jest": "30.0.3",
    "prettier": "3.6.2",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsd": "0.32.0",
    "typescript": "5.8.3"
  }
}
